{
    "manifest_version": 3,
    "name": "DQuest",
    "version": "1.0",
    "description": "My Chrome extension with Lit",
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
  
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["dist/my-element.js"]
    }]
    
    }
