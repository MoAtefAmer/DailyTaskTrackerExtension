{
    "manifest_version": 3,
    "name": "DQuest",
    "version": "1.1.4",
    "description": "Daily Task Tracker",
    "background": {
      "service_worker": "service-worker.js"
      
      
    },
    "action": {
      "default_popup": "popup.html"
    },

    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "permissions": ["storage"],
  
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["dist/app-main.js"]
    }]
    
    }
